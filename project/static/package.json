{
  "name": "mobx-react-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for MobX + React project with JSX, ES6 compilation and hot code reloading",
  "scripts": {
    "lint": "node_modules/.bin/eslint src/** --ext .js,.jsx",
    "build": "node_modules/.bin/webpack --progress --watch",
    "server:dev": "webpack-dev-server --progress --colors --inline --content-base ./dist",
    "start": "npm run server:dev",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mobxjs/mobx-react-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "mobx",
    "starter-kit"
  ],
  "author": "Michel Weststrate <mweststrate@gmail.com> (http://github.com/mweststrate)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mobxjs/mobx/issues"
  },
  "homepage": "http://mobxjs.github.com/mobx",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-regenerator-runtime": "^6.5.0",
    "css-loader": "^0.25.0",
    "eslint": "^2.13.1",
    "eslint-plugin-flowtype": "^2.25.0",
    "eslint-plugin-react": "^6.4.1",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.34.0",
    "flow-status-webpack-plugin": "^0.1.7",
    "jsx-loader": "^0.13.2",
    "node-sass": "^3.10.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "bootstrap": "^4.0.0-alpha.5",
    "computed-async-mobx": "^1.0.0",
    "es6-promise": "^4.0.5",
    "extract-text-webpack-plugin": "^1.0.1",
    "jquery": "^3.1.1",
    "mobx": "^2.2.2",
    "mobx-react": "^3.3.1",
    "mobx-react-devtools": "^4.2.0",
    "rc-slider": "^5.4.3",
    "react": "^15.1.0",
    "react-anything-sortable": "^1.6.1",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.1.0",
    "react-slider": "^0.7.0",
    "react-tagsinput": "^3.13.6",
    "singleton": "^1.0.0",
    "swiper": "^3.4.1",
    "tether": "^1.3.7",
    "underscore": "^1.8.3"
  }
}
